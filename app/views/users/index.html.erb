<div class="row">
	<div class="col-md-6">

		<%= bootstrap_form_tag url: '/login' do |f| %>
		<%= f.text_field :email%>
		<%= f.password_field :password%>
		<%= f.submit "Log In" %>
		<% end %>
	</div>

	<div class="col-md-6">

		<%= bootstrap_form_for(@user) do |f| %>
		<%= f.text_field :email %>
		<%= f.password_field :password %>

		<%= f.submit "Sign Up" %>
		<% end %>
	</div>
</div>